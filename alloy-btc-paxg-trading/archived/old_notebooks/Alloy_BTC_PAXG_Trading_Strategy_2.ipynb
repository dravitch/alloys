    "        last_investment_date = data.index[0]\n",
    "        cash = pd.Series(0.0, index=data.index, dtype=float)\n",
    "        cash.iloc[0] = self.initial_capital\n",
    "        dca_value = pd.Series(0.0, index=data.index, dtype=float)\n",
    "\n",
    "        for i, (date, row) in enumerate(data.iterrows()):\n",
    "            days_since_last = (date - last_investment_date).days\n",
    "            if days_since_last >= 30 and remaining_capital > 0:\n",
    "                investment_amount = min(monthly_investment, remaining_capital)\n",
    "                dca_btc_position += (investment_amount * 0.5) / row['BTC'] if row['BTC'] != 0 else 0\n",
    "                dca_paxg_position += (investment_amount * 0.5) / row['PAXG'] if row['PAXG'] != 0 else 0\n",
    "                remaining_capital -= investment_amount\n",
    "                last_investment_date = date\n",
    "            cash.iloc[i] = remaining_capital\n",
    "            dca_value.iloc[i] = dca_btc_position * row['BTC'] + dca_paxg_position * row['PAXG'] + cash.iloc[i]\n",
    "\n",
    "        results = pd.DataFrame({\n",
    "            'portfolio_value': portfolio_value,\n",
    "            'buy_hold_value': buy_hold_value,\n",
    "            'dca_value': dca_value,\n",
    "            'btc_allocation': btc_alloc_series,\n",
    "            'paxg_allocation': paxg_alloc_series,\n",
    "            'transaction_fees': transaction_fees,\n",
    "            'cumulative_fees': cumulative_fees,\n",
    "            'pnl': portfolio_value - self.initial_capital\n",
    "        }, index=data.index)\n",
    "\n",
    "        self.trades = trades\n",
    "        self.logger.info(\"Backtest completed\")\n",
    "        return results\n",
    "\n",
    "    def calculate_metrics(self, results: pd.DataFrame, historical_data: pd.DataFrame) -> Dict:\n",
    "        if results['portfolio_value'].iloc[0] == 0 or results.empty or len(results) < 2:\n",
    "            self.logger.warning(\"Not enough data or zero initial value for metric calculation.\")\n",
    "            return {\n",
    "                'total_trades': self.trades,\n",
    "                'total_fees': results['cumulative_fees'].iloc[-1] if not results.empty else 0,\n",
    "                'rendement_total_alloy': 0,\n",
    "                'rendement_total_buy_hold': 0,\n",
    "                'rendement_total_dca': 0,\n",
    "                'rendement_annualise_alloy': 0,\n",
    "                'rendement_annualise_buy_hold': 0,\n",
    "                'rendement_annualise_dca': 0,\n",
    "                'volatilite_annualisee_alloy': 0,\n",
    "                'volatilite_annualisee_buy_hold': 0,\n",
    "                'volatilite_annualisee_dca': 0,\n",
    "                'ratio_sharpe_alloy': 0,\n",
    "                'ratio_sharpe_buy_hold': 0,\n",
    "                'ratio_sharpe_dca': 0,\n",
    "                'drawdown_maximum_alloy': 0,\n",
    "                'drawdown_maximum_buy_hold': 0,\n",
    "                'drawdown_maximum_dca': 0,\n",
    "                'drawdown_moyen_alloy': 0,\n",
    "                'drawdown_moyen_btc': 0,\n",
    "                'drawdown_moyen_paxg': 0,\n",
    "                'correlations': None\n",
    "            }\n",
    "\n",
    "        daily_returns = np.log(results['portfolio_value'] / results['portfolio_value'].shift(1)).dropna()\n",
    "        bh_daily_returns = np.log(results['buy_hold_value'] / results['buy_hold_value'].shift(1)).dropna()\n",
    "        dca_daily_returns = np.log(results['dca_value'] / results['dca_value'].shift(1)).dropna()\n",
    "\n",
    "        drawdown = ((results['portfolio_value'].cummax() - results['portfolio_value']) / results['portfolio_value'].cummax()).max() * 100\n",
    "        bh_drawdown = ((results['buy_hold_value'].cummax() - results['buy_hold_value']) / results['buy_hold_value'].cummax()).max() * 100\n",
    "        dca_drawdown = ((results['dca_value'].cummax() - results['dca_value']) / results['dca_value'].cummax()).max() * 100\n",
    "\n",
    "        drawdown_series = (results['portfolio_value'] - results['portfolio_value'].expanding(min_periods=1).max()) / results['portfolio_value'].expanding(min_periods=1).max() * 100\n",
    "        btc_drawdown_series = (historical_data['BTC'] - historical_data['BTC'].expanding(min_periods=1).max()) / historical_data['BTC'].expanding(min_periods=1).max() * 100\n",
    "        paxg_drawdown_series = (historical_data['PAXG'] - historical_data['PAXG'].expanding(min_periods=1).max()) / historical_data['PAXG'].expanding(min_periods=1).max() * 100\n",
    "\n",
    "        correlations = historical_data.pct_change().corr()\n",
    "\n",
    "        metrics = {\n",
    "            'total_trades': self.trades,\n",
    "            'total_fees': results['cumulative_fees'].iloc[-1],\n",
    "            'rendement_total_alloy': (results['portfolio_value'].iloc[-1] / results['portfolio_value'].iloc[0] - 1) * 100,\n",
    "            'rendement_total_buy_hold': (results['buy_hold_value'].iloc[-1] / results['buy_hold_value'].iloc[0] - 1) * 100 if results['buy_hold_value'].iloc[0] != 0 else 0,\n",
    "            'rendement_total_dca': (results['dca_value'].iloc[-1] / results['dca_value'].iloc[0] - 1) * 100 if results['dca_value'].iloc[0] != 0 else 0,\n",
    "            'rendement_annualise_alloy': (daily_returns.mean() * 252) * 100,\n",
    "            'rendement_annualise_buy_hold': (bh_daily_returns.mean() * 252) * 100,\n",
    "            'rendement_annualise_dca': (dca_daily_returns.mean() * 252) * 100,\n",
    "            'volatilite_annualisee_alloy': (daily_returns.std() * np.sqrt(252)) * 100,\n",
    "            'volatilite_annualisee_buy_hold': (bh_daily_returns.std() * np.sqrt(252)) * 100,\n",
    "            'volatilite_annualisee_dca': (dca_daily_returns.std() * np.sqrt(252)) * 100,\n",
    "            'ratio_sharpe_alloy': (daily_returns.mean() * 252) / (daily_returns.std() * np.sqrt(252)) if daily_returns.std() != 0 else 0,\n",
    "            'ratio_sharpe_buy_hold': (bh_daily_returns.mean() * 252) / (bh_daily_returns.std() * np.sqrt(252)) if bh_daily_returns.std() != 0 else 0,\n",
    "            'ratio_sharpe_dca': (dca_daily_returns.mean() * 252) / (dca_daily_returns.std() * np.sqrt(252)) if dca_daily_returns.std() != 0 else 0,\n",
    "            'drawdown_maximum_alloy': drawdown,\n",
    "            'drawdown_maximum_buy_hold': bh_drawdown,\n",
    "            'drawdown_maximum_dca': dca_drawdown,\n",
    "            'drawdown_moyen_alloy': drawdown_series.mean(),\n",
    "            'drawdown_moyen_btc': btc_drawdown_series.mean(),\n",
    "            'drawdown_moyen_paxg': paxg_drawdown_series.mean(),\n",
    "            'correlations': correlations\n",
    "        }\n",
    "        return metrics\n",
    "\n",
    "    def optimize_parameters(self, data: pd.DataFrame, n_trials: int = 50):\n",
    "        \"\"\"Optimize strategy parameters using Optuna to maximize Sharpe ratio.\"\"\"\n",
    "        def objective(trial):\n",
    "            # Define parameter search space\n",
    "            params = {\n",
    "                'momentum_window': trial.suggest_int('momentum_window', 10, 60),\n",
    "                'volatility_window': trial.suggest_int('volatility_window', 20, 90),\n",
    "                'momentum_threshold_bull': trial.suggest_float('momentum_threshold_bull', 0, 20),\n",
    "                'momentum_threshold_bear': trial.suggest_float('momentum_threshold_bear', -20, 0),\n",
    "                'max_btc_allocation': trial.suggest_float('max_btc_allocation', 0.7, 0.95),\n",
    "                'min_btc_allocation': trial.suggest_float('min_btc_allocation', 0.1, 0.4),\n",
    "                'rebalance_frequency': trial.suggest_int('rebalance_frequency', 1, 7)\n",
    "            }\n",
    "\n",
    "            # Ensure min allocation is less than max\n",
    "            if params['min_btc_allocation'] >= params['max_btc_allocation']:\n",
    "                return -np.inf\n",
    "\n",
    "            # Create portfolio with trial parameters\n",
    "            portfolio = AlloyPortfolioBOME(\n",
    "                initial_capital=self.initial_capital,\n",
    "                momentum_window=params['momentum_window'],\n",
    "                volatility_window=params['volatility_window'],\n",
    "                momentum_threshold_bull=params['momentum_threshold_bull'],\n",
    "                momentum_threshold_bear=params['momentum_threshold_bear'],\n",
    "                max_btc_allocation=params['max_btc_allocation'],\n",
    "                min_btc_allocation=params['min_btc_allocation'],\n",
    "                rebalance_frequency=params['rebalance_frequency'],\n",
    "                transaction_cost=self.transaction_cost,\n",
    "                rebalance_thresholds=self.rebalance_thresholds,\n",
    "                observation_periods=self.observation_periods,\n",
    "                data=data\n",
    "            )\n",
    "\n",
    "            # Run backtest\n",
    "            results = portfolio.backtest(data)\n",
    "            metrics = portfolio.calculate_metrics(results, data)\n",
    "\n",
    "            # Return negative Sharpe ratio (to maximize)\n",
    "            return metrics['ratio_sharpe_alloy'] if metrics['ratio_sharpe_alloy'] != 0 else -np.inf\n",
    "\n",
    "        self.logger.info(\"Starting parameter optimization with Optuna...\")\n",
    "        study = optuna.create_study(direction='maximize')\n",
    "        study.optimize(objective, n_trials=n_trials)\n",
    "\n",
    "        best_params = study.best_params\n",
    "        self.logger.info(f\"Best parameters: {best_params}\")\n",
    "        self.params.update(best_params)\n",
    "\n",
    "        # Update portfolio with optimized parameters\n",
    "        self.__init__(\n",
    "            initial_capital=self.initial_capital,\n",
    "            momentum_window=self.params['momentum_window'],\n",
    "            volatility_window=self.params['volatility_window'],\n",
    "            momentum_threshold_bull=self.params['momentum_threshold_bull'],\n",
    "            momentum_threshold_bear=self.params['momentum_threshold_bear'],\n",
    "            max_btc_allocation=self.params['max_btc_allocation'],\n",
    "            min_btc_allocation=self.params['min_btc_allocation'],\n",
    "            rebalance_frequency=self.params['rebalance_frequency'],\n",
    "            transaction_cost=self.transaction_cost,\n",
    "            rebalance_thresholds=self.rebalance_thresholds,\n",
    "            observation_periods=self.observation_periods,\n",
    "            data=self.data,\n",
    "            benchmarks=self.benchmarks\n",
    "        )\n",
    "\n",
    "    def plot_results(self, results: pd.DataFrame, historical_data: pd.DataFrame):\n",
    "        \"\"\"Generate comprehensive visualizations of portfolio performance.\"\"\"\n",
    "        plt.figure(figsize=(15, 12))\n",
    "\n",
    "        # Plot 1: Portfolio Performance\n",
    "        plt.subplot(4, 1, 1)\n",
    "        normalized_portfolio = results['portfolio_value'] / results['portfolio_value'].iloc[0] * 100 if results['portfolio_value'].iloc[0] != 0 else results['portfolio_value'] * 0\n",
    "        normalized_buy_hold = results['buy_hold_value'] / results['buy_hold_value'].iloc[0] * 100 if results['buy_hold_value'].iloc[0] != 0 else results['buy_hold_value'] * 0\n",
    "        normalized_dca = results['dca_value'] / results['dca_value'].iloc[0] * 100 if results['dca_value'].iloc[0] != 0 else results['dca_value'] * 0\n",
    "        normalized_btc = historical_data['BTC'] / historical_data['BTC'].iloc[0] * 100 if historical_data['BTC'].iloc[0] != 0 else historical_data['BTC'] * 0\n",
    "        normalized_paxg = historical_data['PAXG'] / historical_data['PAXG'].iloc[0] * 100 if historical_data['PAXG'].iloc[0] != 0 else historical_data['PAXG'] * 0\n",
    "\n",
    "        plt.plot(normalized_portfolio.index, normalized_portfolio, label='Alloy Portfolio', color='blue')\n",
    "        plt.plot(normalized_buy_hold.index, normalized_buy_hold, label='Buy & Hold', color='orange', linestyle='--')\n",
    "        plt.plot(normalized_dca.index, normalized_dca, label='DCA', color='green', linestyle='-.')\n",
    "        plt.plot(normalized_btc.index, normalized_btc, label='BTC Only', color='red', alpha=0.5)\n",
    "        plt.plot(normalized_paxg.index, normalized_paxg, label='PAXG Only', color='gold', alpha=0.5)\n",
    "        plt.title('Portfolio Performance (Normalized to 100)')\n",
    "        plt.xlabel('Date')\n",
    "        plt.ylabel('Value')\n",
    "        plt.legend()\n",
    "        plt.grid(True)\n",
    "\n",
    "        # Plot 2: Allocations\n",
    "        plt.subplot(4, 1, 2)\n",
    "        plt.plot(results.index, results['btc_allocation'] * 100, label='BTC Allocation', color='blue')\n",
    "        plt.plot(results.index, results['paxg_allocation'] * 100, label='PAXG Allocation', color='gold')\n",
    "        plt.title('Portfolio Allocations Over Time')\n",
    "        plt.xlabel('Date')\n",
    "        plt.ylabel('Allocation (%)')\n",
    "        plt.legend()\n",
    "        plt.grid(True)\n",
    "\n",
    "        # Plot 3: Drawdowns\n",
    "        plt.subplot(4, 1, 3)\n",
    "        portfolio_drawdown = (results['portfolio_value'] - results['portfolio_value'].cummax()) / results['portfolio_value'].cummax() * 100\n",
    "        bh_drawdown = (results['buy_hold_value'] - results['buy_hold_value'].cummax()) / results['buy_hold_value'].cummax() * 100\n",
    "        dca_drawdown = (results['dca_value'] - results['dca_value'].cummax()) / results['dca_value'].cummax() * 100\n",
    "        plt.plot(portfolio_drawdown.index, portfolio_drawdown, label='Alloy Drawdown', color='blue')\n",
    "        plt.plot(bh_drawdown.index, bh_drawdown, label='Buy & Hold Drawdown', color='orange', linestyle='--')\n",
    "        plt.plot(dca_drawdown.index, dca_drawdown, label='DCA Drawdown', color='green', linestyle='-.')\n",
    "        plt.title('Portfolio Drawdowns')\n",
    "        plt.xlabel('Date')\n",
    "        plt.ylabel('Drawdown (%)')\n",
    "