    "import pandas as pd\n",
    "import numpy as np\n",
    "from datetime import datetime, timedelta\n",
    "import matplotlib.pyplot as plt\n",
    "from typing import Dict, List, Tuple, Optional\n",
    "import yfinance as yf\n",
    "import time\n",
    "import logging\n",
    "import sys\n",
    "import os\n",
    "\n",
    "# Configure logging\n",
    "logging.basicConfig(\n",
    "    level=logging.INFO,\n",
    "    format='%(asctime)s - %(levelname)s - %(message)s',\n",
    "    handlers=[\n",
    "        logging.FileHandler('alloy_trading.log'),\n",
    "        logging.StreamHandler(sys.stdout)\n",
    "    ]\n",
    ")\n",
    "logger = logging.getLogger(__name__)\n",
    "\n",
    "def download_and_prepare_data(start_date: str, end_date: str, max_retries: int = 3) -> Optional[pd.DataFrame]:\n",
    "    \"\"\"Downloads and prepares historical BTC and PAXG data with error handling.\"\"\"\n",
    "    def fetch_data(ticker: str) -> pd.Series:\n",
    "        \"\"\"Helper function to download data for a specific ticker.\"\"\"\n",
    "        data = yf.Ticker(ticker)\n",
    "        hist = data.history(start=start_date, end=end_date)\n",
    "        if hist.empty:\n",
    "            raise ValueError(f\"Empty data for {ticker}\")\n",
    "        return hist['Close']\n",
    "\n",
    "    for attempt in range(max_retries):\n",
    "        try:\n",
    "            print(f\"\\nAttempt {attempt + 1}/{max_retries}\")\n",
    "\n",
    "            # Download BTC data\n",
    "            print(\"Downloading BTC...\")\n",
    "            btc_data = fetch_data('BTC-USD')\n",
    "            btc_data.name = 'BTC'\n",
    "\n",
    "            time.sleep(2)  # Delay between requests\n",
    "\n",
    "            # Download PAXG data\n",
    "            print(\"Downloading PAXG...\")\n",
    "            paxg_data = fetch_data('PAXG-USD')\n",
    "            paxg_data.name = 'PAXG'\n",
    "\n",
    "            # Combine data\n",
    "            combined_data = pd.concat([btc_data, paxg_data], axis=1)\n",
    "\n",
    "            # Clean and validate\n",
    "            cleaned_data = combined_data.dropna()\n",
    "            if cleaned_data.empty:\n",
    "                raise ValueError(\"Cleaned data is empty\")\n",
    "\n",
    "            print(f\"\\nData downloaded successfully:\")\n",
    "            print(f\"- Period: {cleaned_data.index[0].strftime('%Y-%m-%d')} to {cleaned_data.index[-1].strftime('%Y-%m-%d')}\")\n",
    "            print(f\"- Data points: {len(cleaned_data)}\")\n",
    "            print(\"\\nLatest prices:\")\n",
    "            print(f\"BTC: ${cleaned_data['BTC'].iloc[-1]:,.2f}\")\n",
    "            print(f\"PAXG: ${cleaned_data['PAXG'].iloc[-1]:,.2f}\")\n",
    "\n",
    "            return cleaned_data\n",
    "\n",
    "        except Exception as e:\n",
    "            print(f\"\\nError on attempt {attempt + 1}: {str(e)}\")\n",
    "            if attempt < max_retries - 1:\n",
    "                wait_time = 5 * (attempt + 1)\n",
    "                print(f\"Retrying in {wait_time} seconds...\")\n",
    "                time.sleep(wait_time)\n",
    "            else:\n",
    "                print(\"\\nFailed to download data after multiple attempts.\")\n",
    "                raise\n",
    "\n",
    "    return None\n",
    "\n",
    "class PortefeuilleAntiFragileTracker:\n",
    "    def __init__(self,\n",
    "                 initial_capital=10000,\n",
    "                 momentum_window=30,\n",
    "                 volatility_window=60,\n",
    "                 momentum_threshold_bull=10,\n",
    "                 momentum_threshold_bear=-5,\n",
    "                 max_btc_allocation=0.9,\n",
    "                 min_btc_allocation=0.2,\n",
    "                 rebalance_frequency=3):\n",
    "        self.capital = initial_capital\n",
    "        self.btc_allocation = 0.5\n",
    "        self.paxg_allocation = 0.5\n",
    "        self.last_rebalance = None\n",
    "        self.positions = {'BTC': 0, 'PAXG': 0}\n",
    "        self.trades_history = []\n",
    "        self.metrics_history = []\n",
    "\n",
    "        # Optimized parameters\n",
    "        self.momentum_window = momentum_window\n",
    "        self.volatility_window = volatility_window\n",
    "        self.momentum_threshold_bull = momentum_threshold_bull\n",
    "        self.momentum_threshold_bear = momentum_threshold_bear\n",
    "        self.max_btc_allocation = max_btc_allocation\n",
    "        self.min_btc_allocation = min_btc_allocation\n",
    "        self.rebalance_frequency = rebalance_frequency\n",
    "\n",
    "    def calculate_volatility(self, prices):\n",
    "        \"\"\"Calculate annualized volatility for given prices.\"\"\"\n",
    "        if isinstance(prices, pd.Series):\n",
    "            returns = np.log(prices / prices.shift(1))\n",
    "            volatility = returns.rolling(window=self.volatility_window).std() * np.sqrt(252)\n",
    "            return volatility.fillna(method='bfill')\n",
    "        return 0\n",
    "\n",
    "    def calculate_momentum(self, prices):\n",
    "        \"\"\"Calculate momentum as percentage change over the momentum window.\"\"\"\n",
    "        return (prices / prices.shift(self.momentum_window) - 1) * 100\n",
    "\n",
    "    def get_market_context(self, data: pd.DataFrame, current_date: datetime) -> Dict:\n",
    "        \"\"\"Generate a summary of the current market context.\"\"\"\n",
    "        btc_data = data['BTC'].loc[:current_date]\n",
    "        paxg_data = data['PAXG'].loc[:current_date]\n",
    "\n",
    "        # Check for sufficient data for momentum\n",
    "        btc_momentum = self.calculate_momentum(btc_data).iloc[-1] if len(btc_data) > self.momentum_window else 0\n",
    "\n",
    "        # Check for sufficient data for volatility\n",
    "        btc_volatility = self.calculate_volatility(btc_data).iloc[-1] if len(btc_data) > self.volatility_window else 0\n",
    "\n",
    "        # Calculate 30-day performance if sufficient data\n",
    "        btc_30d_perf = ((btc_data.iloc[-1] / btc_data.iloc[-30]) - 1) * 100 if len(btc_data) >= 30 else 0\n",
    "        paxg_30d_perf = ((paxg_data.iloc[-1] / paxg_data.iloc[-30]) - 1) * 100 if len(paxg_data) >= 30 else 0\n",
    "\n",
    "        return {\n",
    "            \"date\": current_date,\n",
    "            \"btc_momentum\": btc_momentum,\n",
    "            \"btc_volatility\": btc_volatility,\n",
    "            \"btc_30d_perf\": btc_30d_perf,\n",
    "            \"paxg_30d_perf\": paxg_30d_perf,\n",
    "            \"btc_price\": btc_data.iloc[-1],\n",
    "            \"paxg_price\": paxg_data.iloc[-1]\n",
    "        }\n",
    "\n",
    "    def dynamic_rebalancing(self, current_data, vol_btc, vol_paxg):\n",
    "        \"\"\"Calculate new allocations with justification.\"\"\"\n",
    "        btc_momentum = self.calculate_momentum(current_data['BTC'])\n",
    "        current_btc_momentum = btc_momentum.iloc[-1] if not btc_momentum.empty else 0\n",
    "\n",
    "        old_btc_allocation = self.btc_allocation\n",
    "        decision_reason = \"\"\n",
    "\n",
    "        if current_btc_momentum > self.momentum_threshold_bull:\n",
    "            self.btc_allocation = self.max_btc_allocation\n",
    "            self.paxg_allocation = 1 - self.max_btc_allocation\n",
    "            decision_reason = f\"Strong BTC momentum ({current_btc_momentum:.1f}% > {self.momentum_threshold_bull}%\"\n",
    "\n",
    "        elif current_btc_momentum < self.momentum_threshold_bear:\n",
    "            self.btc_allocation = self.min_btc_allocation\n",
    "            self.paxg_allocation = 1 - self.min_btc_allocation\n",
    "            decision_reason = f\"Weak BTC momentum ({current_btc_momentum:.1f}% < {self.momentum_threshold_bear}%\"\n",
    "\n",
    "        else:\n",
    "            if vol_btc == 0 or vol_paxg == 0:\n",
    "                self.btc_allocation = 0.6\n",
    "                self.paxg_allocation = 0.4\n",
    "                decision_reason = \"Default allocation (missing volatility data)\"\n",
    "            else:\n",
    "                total_vol = vol_btc + vol_paxg\n",
    "                self.btc_allocation = max(self.min_btc_allocation,\n",
    "                                          min(self.max_btc_allocation,\n",
    "                                              1.2 - (vol_btc / total_vol)))\n",
    "                self.paxg_allocation = 1 - self.btc_allocation\n",
    "                decision_reason = f\"Allocation based on relative volatility (BTC: {vol_btc:.1f}%, PAXG: {vol_paxg:.1f}%\"\n",
    "\n",
    "        allocation_change = self.btc_allocation - old_btc_allocation\n",
    "        return decision_reason, allocation_change\n",
    "\n",
    "    def calculate_trades(self, portfolio_value: float, current_prices: Dict[str, float],\n",
    "                         old_allocations: Dict[str, float]) -> List[Dict]:\n",
    "        \"\"\"Calculate trades needed for rebalancing.\"\"\"\n",
    "        trades = []\n",
    "        new_allocations = {'BTC': self.btc_allocation, 'PAXG': self.paxg_allocation}\n",
    "\n",
    "        for asset in ['BTC', 'PAXG']:\n",
    "            current_value = self.positions[asset] * current_prices[asset]\n",
    "            target_value = portfolio_value * new_allocations[asset]\n",
    "            value_difference = target_value - current_value\n",
    "\n",
    "            if abs(value_difference) > portfolio_value * 0.01:  # Minimum threshold 1%\n",
    "                trade_size = abs(value_difference / current_prices[asset])\n",
    "                trade_type = \"BUY\" if value_difference > 0 else \"SELL\"\n",
    "\n",
    "                trades.append({\n",
    "                    'asset': asset,\n",
    "                    'type': trade_type,\n",
    "                    'size': trade_size,\n",
    "                    'price': current_prices[asset],\n",
    "                    'value_usd': abs(value_difference),\n",
    "                    'old_allocation': old_allocations[asset] * 100,\n",
    "                    'new_allocation': new_allocations[asset] * 100\n",
    "                })\n",
    "\n",
    "        return trades\n",
    "\n",
    "    def backtest(self, historical_data: pd.DataFrame) -> Tuple[pd.DataFrame, List[Dict]]:\n",
    "        \"\"\"Execute backtest with detailed rebalancing tracking.\"\"\"\n",
    "        results = []\n",
    "        current_value = self.capital\n",
    "\n",
    "        # Verify data\n",
    "        if historical_data.empty:\n",
    "            raise ValueError(\"Historical data is empty\")\n",
    "\n",
    "        # Initialize positions\n",
    "        initial_btc_price = historical_data['BTC'].iloc[0]\n",
    "        initial_paxg_price = historical_data['PAXG'].iloc[0]\n",
    "        self.positions['BTC'] = (self.capital * self.btc_allocation) / initial_btc_price\n",
    "        self.positions['PAXG'] = (self.capital * self.paxg_allocation) / initial_paxg_price\n",
    "\n",
    "        vol_btc = self.calculate_volatility(historical_data['BTC'])\n",
    "        vol_paxg = self.calculate_volatility(historical_data['PAXG'])\n",
    "        previous_date = None\n",
    "\n",
    "        for date, row in historical_data.iterrows():\n",
    "            if previous_date is None:\n",
    "                previous_date = date\n",
    "\n",
    "            current_prices = {'BTC': row['BTC'], 'PAXG': row['PAXG']}\n",
    "            portfolio_value = sum(pos * price for pos, price in zip(self.positions.values(), current_prices.values()))\n",
    "\n",
    "            days_since_last_rebalance = (date - previous_date).days if self.last_rebalance else self.rebalance_frequency\n",
    "\n",
    "            # Check for rebalancing\n",
    "            if days_since_last_rebalance >= self.rebalance_frequency:\n",
    "                old_allocations = {\n",
    "                    'BTC': self.btc_allocation,\n",
    "                    'PAXG': self.paxg_allocation\n",
    "                }\n",
    "\n",
    "                # Market context\n",
    "                market_context = self.get_market_context(historical_data, date)\n",
    "\n",
    "                # Calculate new allocations\n",
    "                current_vol_btc = vol_btc[date] if isinstance(vol_btc, pd.Series) and date in vol_btc.index else 0\n",
    "                current_vol_paxg = vol_paxg[date] if isinstance(vol_paxg, pd.Series) and date in vol_paxg.index else 0\n",
    "\n",
    "                decision_reason, allocation_change = self.dynamic_rebalancing(\n",
    "                    historical_data.loc[:date],\n",
    "                    current_vol_btc,\n",
    "                    current_vol_paxg\n",
    "                )\n",
    "\n",
    "                # Calculate required trades\n",
    "                trades = self.calculate_trades(portfolio_value, current_prices, old_allocations)\n",
    "\n",
    "                if trades:  # If trades are needed\n",
    "                    # Execute trades\n",
    "                    for trade in trades:\n",
    "                        asset = trade['asset']\n",
    "                        if trade['type'] == \"BUY\":\n",
    "                            self.positions[asset] += trade['size']\n",
    "                        else:\n",
    "                            self.positions[asset] -= trade['size']\n",
    "\n",
    "                    # Record rebalancing\n",
    "                    self.trades_history.append({\n",
    "                        'date': date,\n",
    "                        'portfolio_value': portfolio_value,\n",
    "                        'trades': trades,\n",
    "                        'market_context': market_context,\n",
    "                        'decision_reason': decision_reason\n",
    "                    })\n",
    "\n",
    "                previous_date = date\n",
    "\n",
    "            # Record daily results\n",
    "            results.append({\n",
    "                'date': date,\n",
    "                'portfolio_value': portfolio_value,\n",
    "                'btc_allocation': self.btc_allocation,\n",
    "                'paxg_allocation': self.paxg_allocation\n",
    "            })\n",
    "\n",
    "        return pd.DataFrame(results).set_index('date'), self.trades_history\n",
    "\n",
    "    def generate_rebalancing_report(self, trades_history: List[Dict]) -> str:\n",
    "        \"\"\"Generate a detailed rebalancing report.\"\"\"\n",
    "        report = []\n",
    "        report.append(\"\\n=== REBALANCING REPORT ===\\n\")\n",
    "\n",
    "        for entry in trades_history:\n",
    "            report.append(f\"Date: {entry['date'].strftime('%Y-%m-%d')}\")\n",
    "            report.append(f\"Portfolio value: ${entry['portfolio_value']:,.2f}\")\n",
    "            report.append(f\"\\nMarket context:\")\n",
    "            context = entry['market_context']\n",
    "            report.append(f\"- BTC: ${context['btc_price']:,.2f} (30d: {context['btc_30d_perf']:.1f}%)\")\n",
    "            report.append(f\"- BTC Momentum: {context['btc_momentum']:.1f}%\")\n",
    "            report.append(f\"- BTC Volatility: {context['btc_volatility']:.1f}%\")\n",
    "\n",
    "            report.append(f\"\\nRebalancing reason: {entry['decision_reason']}\")\n",
    "\n",
    "            report.append(\"\\nTrades to execute:\")\n",
    "            for trade in entry['trades']:\n",
    "                report.append(\n",
    "                    f\"- {trade['type']} {trade['asset']}: \"\n",
    "                    f\"{trade['size']:.6f} units @ ${trade['price']:,.2f} \"\n",
    "                    f\"(${trade['value_usd']:,.2f})\"\n",
    "                )\n",
    "                report.append(\n",
    "                    f\"  Allocation {trade['asset']}: \"\n",
    "                    f\"{trade['old_allocation']:.1f}% → {trade['new_allocation']:.1f}%\"\n",
    "                )\n",
    "            report.append(\"\\n\" + \"-\"*50 + \"\\n\")\n",
    "\n",
    "        return \"\\n\".join(report)\n",
    "\n",
    "    def generate_monthly_summary(self, trades_history: List[Dict]) -> str:\n",
    "        \"\"\"Generate a monthly trade summary.\"\"\"\n",
    "        monthly_trades = {}\n",
    "\n",
    "        # Group trades by month\n",
    "        for trade in trades_history:\n",
    "            month_key = trade['date'].strftime('%Y-%m')\n",
    "            if month_key not in monthly_trades:\n",
    "                monthly_trades[month_key] = []\n",
    "            monthly_trades[month_key].append(trade)\n",
    "\n",
    "        summary = []\n",
    "        summary.append(\"\\n=== MONTHLY TRADE SUMMARY ===\\n\")\n",
    "\n",
    "        for month, trades in sorted(monthly_trades.items()):\n",
    "            monthly_btc_volume = sum(abs(t['trades'][0]['value_usd'])\n",
    "                                     for t in trades if t['trades'][0]['asset'] == 'BTC')\n",
    "            monthly_paxg_volume = sum(abs(t['trades'][0]['value_usd'])\n",
    "                                      for t in trades if t['trades'][0]['asset'] == 'PAXG')\n",
    "\n",
    "            # Analyze decisions\n",
    "            decisions = [t['decision_reason'] for t in trades]\n",
    "            momentum_decisions = len([d for d in decisions if 'Momentum' in d])\n",
    "            volatility_decisions = len([d for d in decisions if 'volatility' in d])\n",
    "\n",
    "            summary.append(f\"Month: {month}\")\n",
    "            summary.append(f\"Number of rebalancings: {len(trades)}\")\n",
    "            summary.append(f\"Total traded volume: ${(monthly_btc_volume + monthly_paxg_volume):,.2f}\")\n",
    "            summary.append(\"Decision breakdown:\")\n",
    "            summary.append(f\"- Momentum-based: {momentum_decisions}\")\n",
    "            summary.append(f\"- Volatility-based: {volatility_decisions}\")\n",
    "            summary.append(\"-\" * 50 + \"\\n\")\n",
    "\n",
    "        return \"\\n\".join(summary)\n",
    "\n",
    "    def generate_performance_summary(self, trades_history: List[Dict]) -> str:\n",
    "        \"\"\"Generate a performance summary.\"\"\"\n",
    "        summary = []\n",
    "        summary.append(\"\\n=== PERFORMANCE METRICS ===\\n\")\n",
    "\n",
    "        # Analyze performance\n",
    "        initial_portfolio = trades_history[0]['portfolio_value']\n",
    "        final_portfolio = trades_history[-1]['portfolio_value']\n",
    "        total_return = ((final_portfolio / initial_portfolio) - 1) * 100\n",
    "\n",
    "        # Analyze trades\n",
    "        total_trades = len(trades_history)\n",
    "        momentum_trades = len([t for t in trades_history if 'Momentum' in t['decision_reason']])\n",
    "        volatility_trades = total_trades - momentum_trades\n",
    "\n",
    "        summary.append(f\"Initial capital: ${initial_portfolio:,.2f}\")\n",
    "        summary.append(f\"Final capital: ${final_portfolio:,.2f}\")\n",
    "        summary.append(f\"Total return: {total_return:.2f}%\")\n",
    "        summary.append(f\"\\nTrade statistics:\")\n",
    "        summary.append(f\"Total rebalancings: {total_trades}\")\n",
    "        summary.append(f\"- Momentum-based trades: {momentum_trades}\")\n",
    "        summary.append(f\"- Volatility-based trades: {volatility_trades}\")\n",
    "\n",
    "        return \"\\n\".join(summary)\n",
    "\n",
    "    def generate_trade_alert(self, trade_info: Dict) -> str:\n",
    "        \"\"\"Generate a clear and concise trading alert.\"\"\"\n",
    "        alert = []\n",
    "        alert.append(\"\\n=== TRADING SIGNAL ===\")\n",
    "        alert.append(f\"Date: {trade_info['date'].strftime('%Y-%m-%d')}\")\n",
    "\n",
    "        # Simplified market context\n",
    "        context = trade_info['market_context']\n",
    "        alert.append(\"\\nMARKET CONTEXT:\")\n",
    "        alert.append(f\"BTC/USD: ${context['btc_price']:,.2f} (30d: {context['btc_30d_perf']:.1f}%)\")\n",
    "        alert.append(f\"Momentum: {context['btc_momentum']:.1f}%\")\n",
    "\n",
    "        # Decision\n",
    "        alert.append(f\"\\nDECISION: {trade_info['decision_reason']}\")\n",
    "\n",
    "        # Required actions\n",
    "        alert.append(\"\\nREQUIRED ACTIONS:\")\n",
    "        for trade in trade_info['trades']:\n",
    "            alert.append(\n",
    "                f\"- {trade['type']} {trade['asset']}: \"\n",
    "                f\"{trade['size']:.6f} units @ ${trade['price']:,.2f}\"\n",
    "            )\n",
    "            alert.append(\n",
    "                f\"  → New allocation {trade['asset']}: \"\n",
    "                f\"{trade['new_allocation']:.1f}%\"\n",
    "            )\n",
    "\n",
    "        return \"\\n\".join(alert)\n",
    "\n",
    "    def generate_trading_report(self, backtest_results: pd.DataFrame, trades_history: List[Dict]) -> str:\n",
    "        \"\"\"Generate a comprehensive report with all elements.\"\"\"\n",
    "        report = []\n",
    "\n",
    "        # Header\n",
    "        report.append(\"=\" * 50)\n",
